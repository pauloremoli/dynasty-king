{
  "name": "dynasty-king",
  "private": true,
  "description": "Fantansy Football tools to help you become the King of your dynasty leagues",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:css\" \"netlify dev\"",
    "start": "cross-env NODE_ENV=production  \"npm run dev:css\" \"netlify dev\"",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.3.4",
    "@remix-run/node": "^1.3.4",
    "@remix-run/react": "^1.3.4",
    "@tailwindcss/forms": "^0.5.0",
    "convert-csv-to-json": "^1.3.3",
    "cross-env": "^7.0.3",
    "fuzzysort": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-select-search": "^3.0.9",
    "react-table": "^7.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.4",
    "@remix-run/eslint-config": "^1.3.4",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-table": "^7.7.10",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
